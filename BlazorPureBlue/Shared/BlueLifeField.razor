
@if (IsActive == true)
{
    <input class="form-check" type="checkbox" value=@IsChecked/>
}
else
{
    <input class="form-check" type="checkbox" value=@IsChecked disabled />
}

@code {
    [Parameter]
    public bool IsChecked { get; set; }
    [Parameter]
    public bool IsActive { get; set; }


}
