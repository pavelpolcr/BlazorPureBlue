
@if (IsActive == true)
{
    <input class="form-check" type="checkbox" @bind=Model.IsChecked onclick="Clicktest"/>
}
else
{
    <input class="form-check" type="checkbox" @bind=Model.IsChecked onclick="Clicktest" disabled />
}

@code {
    
    [Parameter]
    public bool IsActive { get; set; }
    [Parameter]
    public BlueLifeFieldModel Model { get; set; }



}
